{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "title": "User",
    "type": "object",
    "properties": {
        "uid": {
            "type": "integer"
        },
        "username": {
            "type": "string"
        },
        "avatar": {
            "type": "string"
        },
        "user_type": {
            "type": "string",
            "enum": ["NORMAL", "ADVANCED", "ADMIN"]
        },
        "create_time": {
            "type": "integer"
        }
    },
    "required": [
        "avatar",
        "create_time",
        "uid",
        "user_type",
        "username"
    ]
}
